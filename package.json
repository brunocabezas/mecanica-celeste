{
  "name": "mecanica-celeste-webapp",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development parcel src/index.html",
    "prebuild": "rm -rf parcel-build",
    "build": "NODE_ENV=production parcel build src/index.html --out-dir parcel-build",
    "lint": "npx eslint src/",
    "test": "node scripts/test.js --env=jsdom"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": "^6.14.4"
  },
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "jeet": "^7.2.0",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dev-utils": "^7.0.5",
    "react-dom": "^16.14.0",
    "react-graph-vis": "^1.0.5",
    "react-loadable": "^5.5.0",
    "react-player": "^1.15.3",
    "react-responsive-modal": "^3.6.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^7.0.2",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-eslint": "^1.0.7",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
